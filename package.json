{
  "name": "envisionbasic",
  "displayName": "Envision Basic Language Support",
  "description": "Unibasic language support with Envision extensions by Ellucian",
  "icon": "images/e-go-icon.png",
  "version": "0.0.1",
  "publisher": "konradwillmert",
  "bugs": {
    "url": "https://github.com/Icebird/envisionbasic/issues"
  },
  "homepage": "https://github.com/Icebird/envisionbasic/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Icebird/envisionbasic.git"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "eb",
        "aliases": [
          "Envision Basic",
          "eb"
        ],
        "extensions": [
          ".ub",
          ".eb",
          ".ebl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eb",
        "scopeName": "source.envisionbasic",
        "path": "./syntaxes/eb.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "eb",
        "path": "./snippets/eb.json"
      }
    ]
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-convert": "^0.2.0",
    "gulp-ext-replace": "^0.3.0"
  }
}
